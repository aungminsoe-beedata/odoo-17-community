<odoo>
    <data noupdate="1">

        <!-- Sequence for RFQ -->
        <record id="seq_rfq" model="ir.sequence">
            <field name="name">RFQ Sequence</field>
            <field name="code">purchase.order.rfq</field>
            <field name="prefix">RFQ</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Sequence for PO -->
        <record id="seq_po" model="ir.sequence">
            <field name="name">PO Sequence</field>
            <field name="code">purchase.order.po</field>
            <field name="prefix">PO</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>
</odoo>
